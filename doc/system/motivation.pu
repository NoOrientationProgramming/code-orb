
@startuml

' skinparam linetype ortho
' scale 0.6

component PC as pPc {

	frame "Debugger" as pDbg
	frame "Application" as pApp

	pDbg <-[thickness=2]-> pApp : "  GDB\n  Logs\n  IPC"
}

component "Microcontroller" {

	frame "<<Application>>" as pAppUc
}

pDbg <-> pAppUc : "    (GDB), UART (Logs)  \nGPIO"

footer **CodeOrb** - Motivation\n(C) DSP-Crowd Electronics GmbH

@enduml

